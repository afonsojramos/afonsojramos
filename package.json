{
  "name": "afonsojramos",
  "version": "1.0.10",
  "description": "A personal command-line business card.",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.json",
    "dev": "tsc-watch -p tsconfig.json --onSuccess 'node ./dist/index.js'",
    "lint": "eslint .",
    "test": "mocha",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/clear": "^0.1.2",
    "@types/inquirer": "^8.1.3",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "boxen": "^6.2.1",
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "inquirer": "^8.2.0",
    "open": "^8.4.0"
  },
  "keywords": [
    "business",
    "card",
    "npx card",
    "business card"
  ],
  "author": "Afonso Ramos <afonsojorgeramos@gmail.com>",
  "homepage": "https://afonsojramos.me",
  "license": "ISC",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "afonsojramos": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afonsojramos/afonsojramos.git"
  },
  "bugs": {
    "url": "https://github.com/afonsojramos/afonsojramos/issues"
  }
}
